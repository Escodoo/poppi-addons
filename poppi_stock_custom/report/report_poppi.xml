<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data>
        <record id="action_report_rotulo" model="ir.actions.report">
            <field name="name">Rótulo de Entrega</field>
            <field name="model">stock.picking</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">poppi_stock_custom.poppi_rotulo_report</field>
            <field name="report_file">poppi_stock_custom.poppi_rotulo_report</field>
            <field
                name="print_report_name"
            >'Rótulo de Entrega - %s - %s' % (object.partner_id.name or
                '', object.name)</field>
            <field name="binding_model_id" ref="stock.model_stock_picking" />
            <field name="binding_type">report</field>
        </record>
    </data>
</odoo>
